# E2E Smoke Variables

# Required for local Vite app auth bootstrap (when E2E_BASE_URL is omitted)
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Optional. If omitted, Playwright starts local Vite at http://127.0.0.1:4173
E2E_BASE_URL=

# PT smoke account
E2E_PT_EMAIL=
E2E_PT_PASSWORD=

# Client smoke account
E2E_CLIENT_EMAIL=
E2E_CLIENT_PASSWORD=

# Existing client UUID visible to PT account
E2E_CLIENT_ID=

# Existing workout template UUID in same workspace as E2E_CLIENT_ID
E2E_WORKOUT_TEMPLATE_ID=
